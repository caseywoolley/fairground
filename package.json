{
  "name": "fairground",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "chain": "npx hardhat node",
    "compile": "npx hardhat compile",
    "localhost": "npx hardhat run scripts/deploy/localhost.ts --network localhost",
    "mainnet": "npx hardhat run scripts/deploy/mainnet.ts --network mainnet",
    "mumbai": "npx hardhat run scripts/deploy/mumbai.ts --network mumbai",
    "goerli": "npx hardhat run scripts/deploy/goerli.ts --network goerli",
    "polygon": "npx hardhat run scripts/deploy/polygon.ts --network polygon",
    "dev": "cd frontend && yarn dev && cd..",
    "devploy": "yarn localhost && yarn dev",
    "build": "cd frontend && yarn build && cd..",
    "test": "hardhat test"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/providers": "^5.7.0",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.3",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.5",
    "@nomicfoundation/hardhat-toolbox": "^1.0.2",
    "@nomiclabs/hardhat-ethers": "^2.1.1",
    "@nomiclabs/hardhat-etherscan": "^3.1.0",
    "@openzeppelin/contracts": "^4.7.3",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.2",
    "@types/chai": "^4.3.3",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.7.14",
    "chai": "^4.3.6",
    "dotenv": "^16.0.3",
    "ethers": "^5.7.0",
    "hardhat": "^2.9.9",
    "hardhat-gas-reporter": "^1.0.8",
    "solidity-coverage": "^0.7.21",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.0",
    "typescript": "^4.8.2"
  }
}
